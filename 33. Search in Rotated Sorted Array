class Solution(object):
    def search(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: int
        """
        if len(nums) == 0:
            return -1
        l = 0
        r = len(nums)-1
        while l <= r:
            mid = int((r+l)/2)
            if nums[mid] == target:
                return mid
            if nums[mid] >= nums[l]:
                if nums[l]<= target <=nums[mid]:
                    r = mid - 1
                else:
                    l = mid + 1
            else:
                if nums[mid]<= target <=nums[r]:
                    l = mid + 1
                else:
                    r = mid - 1
                    
        return -1
